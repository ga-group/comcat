	skos:inScheme	ccat: ;
	skos:notation	"10" ;
	skos:prefLabel	"Hards"@en ;
	skos:topConceptOf	ccat: .

ccat:1010
	a	skos:Concept ;
	skos:broader	ccat:10 ;
	skos:closeMatch	stw:18996-5 , wd:Q1239166 ;
